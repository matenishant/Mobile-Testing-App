<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nishant.com.touchchecker">

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.USE_FINGERPRINT" />
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />
    <uses-feature android:name="android.hardware.camera" android:required="true"/>



    <uses-sdk android:minSdkVersion="17"
        android:targetSdkVersion="25"
        android:maxSdkVersion="25" />

<supports-screens android:xlargeScreens="true"/>
    <supports-screens android:normalScreens="true"/>
    <supports-screens android:resizeable="true"/>
    <supports-screens android:smallScreens="true"/>



    <application
        android:anyDensity="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name="nishant.com.touchchecker.MainActivity">


            </activity>
        <activity android:name="nishant.com.touchchecker.Red" />
        <activity android:name="nishant.com.touchchecker.Green" />
        <activity android:name="nishant.com.touchchecker.Blue" />
        <activity android:name="nishant.com.touchchecker.Proximity" />
        <activity android:name="nishant.com.touchchecker.Dimming" />
        <activity android:name="nishant.com.touchchecker.Touch1" />
        <activity android:name="nishant.com.touchchecker.Subkey" />
        <activity android:name="nishant.com.touchchecker.ThumbImpression" />
        <activity android:name="nishant.com.touchchecker.Flashlight" />
        <activity
            android:name="nishant.com.touchchecker.Splash_Screen"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">


            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>


        </activity>
        <activity android:name="nishant.com.touchchecker.Systeminfo" />
        <activity android:name="nishant.com.touchchecker.RecordPlay" />
        <activity android:name="nishant.com.touchchecker.TiltBall"></activity>
    </application>

</manifest>